    "def digital_root(n):\n",
    "    sum_n = sum(list(map(int, str(n))))\n",
    "\n",
    "    while sum_n > 9:\n",
    "        sum_n = sum(list(map(int, str(sum_n))))\n",
    "\n",
    "    return (sum_n)"
