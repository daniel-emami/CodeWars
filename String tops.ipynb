   "source": [
    "def tops(msg):\n",
    "    i = 0\n",
    "    peaks = []\n",
    "\n",
    "    for idx in range(len(msg)):\n",
    "        i += idx\n",
    "        if i > len(msg):\n",
    "            break\n",
    "        if idx % 2 == 1:\n",
    "            peaks.append(msg[i])\n",
    "\n",
    "    return ''.join(peaks[::-1])"
   ]
  }
