   "source": [
    "import itertools\n",
    "\n",
    "\n",
    "def permutations(s):\n",
    "    permutations = list(set(itertools.permutations(s, len(s))))\n",
    "    \n",
    "    return [''.join(i) for i in permutations]\n"
   ]
  }
